function (rna.id = opts$rna_cells, met.id = opts$met_cells, met.anno = opts$met.annos[1], 
    acc.id = opts$acc.annos[1], acc.anno = met.anno, min.cpg = 5, 
    min.gpc = 5, io = io) 
{
    ml2map <- function(dt) {
        dt[, `:=`(rate, rate/100)]
        dt[, `:=`(rate, (rate * N + 1)/(N + 2))]
        dt
    }
    met_dt <- fread(cmd = sprintf("zcat < %s/%s.tsv.gz", io$met.dir, 
        met.anno), stringsAsFactors = F, quote = "", showProgress = F) %>% 
        setnames(c("id_met", "id", "anno", "Nmet", "N", "rate")) %>% 
        ml2map()
    acc_dt <- fread(cmd = sprintf("zcat < %s/%s.tsv.gz", io$acc.dir, 
        acc.anno), stringsAsFactors = F, quote = "", showProgress = F) %>% 
        setnames(c("id_acc", "id", "anno", "Nmet", "N", "rate")) %>% 
        ml2map()
    sce <- readRDS(io$rna.file)
    sce <- sce[, colnames(sce) %in% rna.id]
    sce <- sce[rowMeans(logcounts(sce)) >= 0.3, ]
    met_dt <- met_dt[id_met %in% met.id] %>% setnames("rate", 
        "value")
    acc_dt <- acc_dt[id_acc %in% acc.id] %>% setnames("rate", 
        "value")
    met_dt <- met_dt[N >= min.cpg]
    acc_dt <- acc_dt[N >= min.gpc]
    return(list(met = met_dt, acc = acc_dt, rna = rna_dt))
}

