function (dt_list, row = "id", col = "id_met", value.var = "rate") 
{
    lapply(dt_list, function(w) {
        dcast.data.table(w, as.formula(sprintf("%s~%s", row, 
            col)), value.var = value.var) %>% data.frame(row.names = TRUE, 
            check.names = FALSE) %>% .[lci_sorted_feats(w), ] %>% 
            as.matrix()
    })
}

